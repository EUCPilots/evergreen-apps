{
	"Name": "FreeFem",
	"Source": "https://freefem.org/",

	"Get": {
		"Uri": "https://api.github.com/repos/FreeFem/FreeFem-sources/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$"
	},
	"Install": {
		"Setup": "FreeFEM*.exe",
		"Physical": {
			"Arguments": "/VERYSILENT /NORESTART /NOCANCEL /SP-",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/VERYSILENT /NORESTART /NOCANCEL /SP-",
			"PostInstall": []
		}
	}
}
