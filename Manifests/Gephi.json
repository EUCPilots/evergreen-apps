{
	"Name": "Gephi",
	"Source": "https://gephi.org/",

	"Get": {
		"Uri": "https://api.github.com/repos/gephi/gephi/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.*exe$"
	},
	"Install": {
		"Setup": "gephi*.exe",
		"Physical": {
			"Arguments": "/VERYSILENT",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/VERYSILENT",
			"PostInstall": []
		}
	}
}
