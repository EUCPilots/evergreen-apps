{
	"Name": "KeeWeb",
	"Source": "https://github.com/keeweb/keeweb",
	"Get": {
		"Uri": "https://api.github.com/repos/keeweb/keeweb/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$|\\.zip$"
	},
	"Install": {
		"Setup": "KeeWeb*.exe",
		"Physical": {
			"Arguments": "/NCRC /S",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "",
			"PostInstall": []
		}
	}
}
