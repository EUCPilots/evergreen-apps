{
	"Name": "Maximus5 ConEmu",
	"Source": "https://conemu.github.io/",
	"Get": {
		"Uri": "https://api.github.com/repos/ConEmu/ConEmu/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$|\\.7z$"
	},
	"Install": {
		"Setup": "ConEmuSetup.*.exe",
		"Physical": {
			"Arguments": "/p:x64,adm /qn",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "",
			"PostInstall": []
		}
	}
}
