{
	"Name": "Microsoft Bicep",
	"Source": "https://docs.microsoft.com/en-us/azure/azure-resource-manager/bicep/overview",
	"Get": {
		"Uri": "https://api.github.com/repos/azure/bicep/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$|\\.msi$"
	},
	"Install": {
		"Setup": "bicep-setup*.exe",
		"Preinstall": "",
		"Physical": {
			"Arguments": "/install /quiet",
			"PostInstall": [
			]
		},
		"Virtual": {
			"Arguments": "/install /quiet",
			"PostInstall": [
			]
		}
	}
}
