{
	"Name": "NETworkManager",
	"Source": "https://github.com/BornToBeRoot/NETworkManager",
	"Get": {
		"Uri": "https://api.github.com/repos/borntoberoot/networkmanager/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.msi$"
	},
	"Install": {
		"Setup": "NETworkManager_*_Setup.msi",
		"Physical": {
			"Arguments": "/qn /norestart",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/qn /norestart",
			"PostInstall": []
		}
	}
}
