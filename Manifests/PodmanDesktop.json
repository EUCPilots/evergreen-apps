{
	"Name": "Podman Desktop",
	"Source": "https://github.com/podman-desktop/podman-desktop",
	"Get": {
		"Uri": "https://api.github.com/repos/podman-desktop/podman-desktop/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$"
	},
	"Install": {
		"Setup": "podman-desktop*.exe",
		"Physical": {
			"Arguments": "/S",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "",
			"PostInstall": []
		}
	}
}
