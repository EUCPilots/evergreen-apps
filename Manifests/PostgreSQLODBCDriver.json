	"Get": {
		"Uri": "https://api.github.com/repos/postgresql-interfaces/psqlodbc/releases/latest",
		"MatchVersion": "(?<=REL-)(\\d{2}_\\d{2}_\\d{4})",
		"MatchFileTypes": "\\.exe$|\\.msi$"
	}
}
