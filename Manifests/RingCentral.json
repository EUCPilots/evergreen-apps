	"Get": {
		"Update": {
			"Uri": "https://app.ringcentral.com/download/latest.yml"
		},
		"Download": {
			"Windows": {
				"Exe": "https://app.ringcentral.com/download/RingCentral.exe",
                "Msi": "https://app.ringcentral.com/download/RingCentral-x64.msi",
                "Meetings": "https://downloads.ringcentral.com/RCM/RC/meetings/win/RCMeetingsClientSetup.msi",
				"Outlook": "https://downloads.ringcentral.com/RCM/RC/meetings/OutlookPlugin/RCMeetingsOutlookPluginSetup.msi",
				"Rooms": "https://downloads.ringcentral.com/RCM/RC/rooms/win/RingCentralRoomsSetup.msi"
			}
        },
        "MatchYmlVersion": "^version:\\s(\\d+(\\.\\d+){1,4})",
		"MatchFileVersion": "(\\d+(\\.\\d+){1,4})",
		"MatchUrl": "(.*)\\?"
	},
	"Install": {
		"Setup": "AppName.*.exe",
		"Physical": {
			"Arguments": "",
			"PostInstall": [

			]
		},
		"Virtual": {
			"Arguments": "",
			"PostInstall": [

			]
		}
	}
}
