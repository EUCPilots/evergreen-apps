{
	"Name": "ShareX",
	"Source": "https://getsharex.com/",
	"Get": {
		"Uri": "https://api.github.com/repos/ShareX/ShareX/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$|\\.msi$|\\.msp$|\\.zip$"
	},
	"Install": {
		"Setup": "ShareX*.exe",
		"Physical": {
			"Arguments": "/VERYSILENT /NORUN",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/VERYSILENT /NORUN",
			"PostInstall": []
		}
	}
}
