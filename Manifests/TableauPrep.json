{
	"Name": "Tableau Prep",
	"Source": "https://www.tableau.com/",
	"Get": {
		"Update": {
			"Uri": ""
		},
		"Download": {
			"Uri": "https://www.tableau.com/downloads/prep/pc64",
			"Property": "ResponseUri.Headers.Location",
			"MatchVersion": "(\\d+(\\-\\d+){1,3})",
			"Headers": {
                "sec-fetch-dest": "document",
                "accept-language": "en-AU,en-GB;q=0.9,en;q=0.8,en-US;q=0.7",
                "sec-fetch-mode": "navigate",
                "dnt": "1",
                "method": "GET",
                "sec-ch-ua-mobile": "?0",
                "scheme": "https",
                "sec-ch-ua-platform": "\"macOS\"",
                "authority": "www.tableau.com",
                "sec-ch-ua": "\"Chromium\";v=\"132\", \"Microsoft Edge\";v=\"131\", \"Not-A.Brand\";v=\"99\"",
                "upgrade-insecure-requests": "1",
                "accept-encoding": "gzip, deflate, br, zstd",
                "sec-fetch-site": "none",
                "priority": "u=0, i",
                "path": "/downloads/desktop/pc64",
                "accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
                "sec-fetch-user": "?1"
            }
		}
	},
	"Install": {
		"Setup": "TableauPrep*.exe",
		"Physical": {
			"Arguments": "/quiet /norestart",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/quiet /norestart",
			"PostInstall": []
		}
	}
}
