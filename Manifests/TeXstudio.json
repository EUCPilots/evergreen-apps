{
	"Name": "TeXstudio",
	"Source": "https://www.texstudio.org/",

	"Get": {
		"Uri": "https://api.github.com/repos/texstudio-org/texstudio/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$"
	},
	"Install": {
		"Setup": "TeXstudio*.exe",
		"Physical": {
			"Arguments": "/S",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/S",
			"PostInstall": []
		}
	}
}
