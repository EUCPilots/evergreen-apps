{
	"Name": "WinMerge",
	"Source": "https://winmerge.org/",
        
	"Get": {
		"Uri": "https://api.github.com/repos/WinMerge/winmerge/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.msi$|\\.exe$"
	},
	"Install": {
		"Setup": "WinMerge*.exe",
		"Physical": {
			"Arguments": "/Silent",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/Silent",
			"PostInstall": []
		}
	}
}
