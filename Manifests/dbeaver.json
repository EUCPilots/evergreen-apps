{
	"Name": "dbeaver",
	"Source": "https://github.com/dbeaver/dbeaver",
	"Get": {
		"Uri": "https://api.github.com/repos/dbeaver/dbeaver/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$"
	},
	"Install": {
		"Setup": "dbeaver-ce-*-x86_64-setup.exe",
		"Physical": {
			"Arguments": "/allusers /S",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/allusers /S",
			"PostInstall": []
		}
	}
}
